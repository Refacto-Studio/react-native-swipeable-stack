{
  "name": "react-native-swipeable-stack",
  "version": "1.0.0",
  "description": "A performant, customizable Tinder-like swipeable card stack for React Native with Reanimated 3+ and Gesture Handler",
  "main": "lib/commonjs/index.js",
  "module": "lib/module/index.js",
  "types": "lib/typescript/src/index.d.ts",
  "react-native": "src/index.ts",
  "source": "src/index.ts",
  "exports": {
    ".": {
      "import": {
        "types": "./lib/typescript/src/index.d.ts",
        "default": "./lib/module/index.js"
      },
      "require": {
        "types": "./lib/typescript/src/index.d.ts",
        "default": "./lib/commonjs/index.js"
      }
    }
  },
  "files": [
    "src",
    "lib",
    "!**/__tests__",
    "!**/__fixtures__",
    "!**/__mocks__"
  ],
  "scripts": {
    "prepare": "bob build",
    "clean": "del-cli lib",
    "typescript": "tsc --noEmit",
    "lint": "eslint \"src/**/*.{ts,tsx}\"",
    "test": "jest"
  },
  "keywords": [
    "react-native",
    "swipe",
    "tinder",
    "card",
    "stack",
    "gesture",
    "reanimated",
    "animation"
  ],
  "author": "Louis Capelle",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/Refacto-Studio/react-native-swipeable-stack"
  },
  "bugs": {
    "url": "https://github.com/Refacto-Studio/react-native-swipeable-stack/issues"
  },
  "homepage": "https://github.com/Refacto-Studio/react-native-swipeable-stack#readme",
  "peerDependencies": {
    "react": ">=18.0.0",
    "react-native": ">=0.70.0",
    "react-native-gesture-handler": ">=2.0.0",
    "react-native-reanimated": ">=3.0.0"
  },
  "devDependencies": {
    "@types/react": "^18.0.0",
    "@types/react-native": "^0.70.0",
    "del-cli": "^5.1.0",
    "eslint": "^8.0.0",
    "jest": "^29.0.0",
    "react": "^18.0.0",
    "react-native": "^0.72.0",
    "react-native-builder-bob": "^0.23.0",
    "react-native-gesture-handler": "^2.12.0",
    "react-native-reanimated": "^3.5.0",
    "typescript": "^5.0.0"
  },
  "react-native-builder-bob": {
    "source": "src",
    "output": "lib",
    "targets": [
      [
        "commonjs",
        {
          "esm": true
        }
      ],
      [
        "module",
        {
          "esm": true
        }
      ],
      [
        "typescript",
        {
          "project": "tsconfig.build.json"
        }
      ]
    ]
  }
}
